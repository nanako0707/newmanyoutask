<h1><%= t('view.admin_screen')%>: <%= t('view.user_list')%></h1>

<% @users.each do |user| %>
  <%= user.id %>: <%= user.name %>
  <%= link_to t('view.user_delete'), [:admin, user], method: :delete,
                                           data: { confirm: t('view.confirm')} %>
<% end %>
<%= paginate (@users) %>
